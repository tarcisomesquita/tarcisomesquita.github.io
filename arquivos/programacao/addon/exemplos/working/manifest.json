{
  "manifest_version": 3,
  "name": "P2SED",
  "version": "1.0",
  "description": "Transfere acesso da SED para uma planilha fazer lançamentos no diário automaticamente",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://sed.educacao.sp.gov.br/Inicio"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Enviar Acesso SED"
  }
}